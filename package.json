{
  "name": "capif-back",
  "version": "v0.3.10",
  "main": "dist/app.js",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "clean:dist": "rimraf dist",
    "clean:uploads": "rimraf uploads",
    "postgres:init": "node dist/config/database/initPostgres.js",
    "redis": "node dist/config/redis.js",
    "start": "node dist/app.js",
    "dev.local:up": "docker compose --env-file .env.dev.local -f compose.dev.local.yml up --build -d",
    "dev.local:down": "docker compose --env-file .env.dev.local -f compose.dev.local.yml down -v",
    "dev.local:restart": "npm run dev.local:down && npm run dev.local:up",
    "prod.local:up": "docker compose --env-file .env.prod.local -f compose.prod.local.yml up --build -d",
    "prod.local:down": "docker compose --env-file .env.prod.local -f compose.prod.local.yml down -v && docker rmi javiertrombetta/capif-back:latest",
    "prod.local:restart": "npm run prod.local:down && npm run prod.local:up",
    "init": "npm run dev.local:down && npm run clean:dist && npm run clean:uploads && npm run dev.local:up && npm run build && cross-env NODE_ENV=development npm run postgres:init",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec ts-node src/app.ts",
    "debug": "cross-env NODE_ENV=development nodemon --watch src --exec node --inspect -r ts-node/register src/app.ts",
    "test": "echo \"No tests defined yet\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "archiver": "^7.0.1",
    "bcrypt": "^5.1.1",
    "celebrate": "^15.0.3",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "fs-extra": "^11.3.0",
    "ftp": "^0.3.10",
    "ftp-srv": "^4.6.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.8",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "path": "^0.12.7",
    "pg": "^8.11.0",
    "redis": "^4.7.0",
    "sequelize": "^6.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cls-hooked": "^4.3.9",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/ftp": "^0.3.36",
    "@types/ioredis": "^4.28.10",
    "@types/joi": "^17.2.2",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.12.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.11.10",
    "@types/redis": "^4.0.10",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
