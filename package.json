{
  "name": "capif-back",
  "version": "v0.1.13",
  "main": "dist/app.js",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "clean:dist": "rimraf dist",
    "clean:postgres": "rimraf postgres",
    "init": "node dist/config/database/init.js",
    "start": "node dist/app.js",
    "dev.compose:up": "docker compose --env-file .env.dev.local -f compose.dev.local.yml up --build -d",
    "dev.compose:down": "docker compose --env-file .env.dev.local -f compose.dev.local.yml down -v",
    "prod.local:up": "docker compose --env-file .env.prod.local -f compose.prod.local.yml up --build -d",
    "prod.local:down": "docker compose --env-file .env.prod.local -f compose.prod.local.yml down -v && docker rmi javiertrombetta/capif-back:latest",
    "prod.local": "npm run prod.local:down && npm run prod.local:up",
    "dev": "NODE_ENV=development npm run dev.compose:down && npm run clean:dist && npm run dev.compose:up && npm run build && npm run init && nodemon --watch src --exec ts-node src/app.ts",
    "test": "echo \"No tests defined yet\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "celebrate": "^15.0.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.15",
    "pg": "^8.11.0",
    "redis": "^4.7.0",
    "sequelize": "^6.37.3",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/ioredis": "^4.28.10",
    "@types/joi": "^17.2.2",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.11.10",
    "@types/redis": "^4.0.10",
    "@types/sequelize": "^4.28.20",
    "@types/serve-favicon": "^2.5.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.9.1",
